                        <ending xml:id="e6ixynw" lendsym="angledown" n="2">
                            <measure xml:id="m1sthixk" right="end" n="18">
                                <staff xml:id="srnxwnd" n="1">
                                    <layer xml:id="l22g1pu" n="1">
                                        <note xml:id="n1o8d3sa" dur.ppq="1" dur="4" oct="4" pname="a" stem.dir="up"/>
