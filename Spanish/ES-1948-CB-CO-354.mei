                            <dir xml:id="d1d8ejqw" staff="1" tstamp="3.000000" vgrp="40">
                                Allá alante, allá alante,
                                <lb xml:id="lusta6y"/>
                                <lb xml:id="l1y8vsm4"/>
                                hay un ciego naranjero;
                                <lb xml:id="l1phap0q"/>
                                <lb xml:id="l1255gpf"/>
                                ciego, dame una naranja
                                <lb xml:id="l1batif5"/>
                                <lb xml:id="l466li8"/>
                                para el Niño entretenerlo.
                                <lb xml:id="ld5a3mu"/>
                                <lb xml:id="les3qce"/>
                                <lb xml:id="l1wdnvex"/>
                                Entre usted, señora, y corte
                                <lb xml:id="lo55kig"/>
                                <lb xml:id="l1o8zvhi"/>
                                las que sean menester;
                                <lb xml:id="l41ojn1"/>
                                <lb xml:id="l7lsqf9"/>
                                entre usted, señor, y corte,
                                <lb xml:id="l8e292x"/>
                                <lb xml:id="l7bpk23"/>
                                las que sean menester.
                                <lb xml:id="l1lsrdcy"/>
                                <lb xml:id="lfoc7dq"/>
                                <lb xml:id="ldb88ak"/>
                                Corto una para el Niño
                                <lb xml:id="l4ybcio"/>
                                <lb xml:id="l1yl8i4v"/>
                                y otra para San José;
                                <lb xml:id="l1u0jstq"/>
                                <lb xml:id="le2fqs7"/>
                                cortaba una por una
                                <lb xml:id="l1m25lo1"/>
                                <lb xml:id="losqbeu"/>
                                y nacían tres en tres.
                                <lb xml:id="lk5d3x8"/>
                                <lb xml:id="lmdbarq"/>
                                <lb xml:id="l71ycy6"/>
                                Ya se va aquella señora,
                                <lb xml:id="l1nc6suv"/>
                                <lb xml:id="lfhtcwe"/>
                                ya se va aquella mujer,
                                <lb xml:id="l10g4u34"/>
                                <lb xml:id="l13m1qcc"/>
                                que me dió vista a los ojos
                                <lb xml:id="lhrk3h7"/>
                                <lb xml:id="l17xkhkf"/>
                                y a mi corazón también.
                                <lb xml:id="l3dr9bc"/>
                                <lb xml:id="l1pnflkx"/>
                            </dir>
