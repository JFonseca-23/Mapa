                                <dir xml:id="d1u6bpyg" staff="1" tstamp="1.000000" vgrp="40">
                                    <lb xml:id="ljz2qud"/>
                                    Quan van tenir carn
                                    <lb xml:id="l17sje6o"/>
                                    els falta cuinera.
                                    <lb xml:id="lmoy3me"/>
                                    Ja en va eixir la xinxa
                                    <lb xml:id="lpqrpz4"/>
                                    de sota l'aiguera:
                                    <lb xml:id="l1y2p99h"/>
                                    <lb xml:id="lgag1re"/>
                                    — Que's facen les bodes:
                                    <lb xml:id="l1drzawc"/>
                                    jo en seré cuinera. —
                                    <lb xml:id="l1jh3mnr"/>
                                    Quan tenen cuinera
                                    <lb xml:id="l1k31geg"/>
                                    els manca criada.
                                    <lb xml:id="l61ycim"/>
                                    <lb xml:id="lbf3at9"/>
                                    Ja va eixir la rata
                                    <lb xml:id="l1j3terv"/>
