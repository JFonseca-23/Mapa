                                    <lb xml:id="la69ifl"/>
                                    <lb xml:id="l1pxhsvh"/>
                                    Quan tenen les bodes
                                    <lb xml:id="lph3fgm"/>
                                    els falta music.
                                    <lb xml:id="l17pjjrg"/>
