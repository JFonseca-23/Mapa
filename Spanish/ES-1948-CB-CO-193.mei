                                                la
                                            </syl>
                                        </verse>
                                    </note>
                                    <note xml:id="naj46b3" dur.ppq="2" dur="4" oct="4" pname="g" stem.dir="up">
                                        <verse xml:id="v1o8g9f2" n="1">
                                            <syl xml:id="s1p0zr13" con="s" wordpos="t">
