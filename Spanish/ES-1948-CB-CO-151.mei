                            <dir xml:id="d5y4bqv" place="below" staff="1" tstamp="2.500000" vgrp="40">
                                Se iba de paseo
                                <lb xml:id="lxs5g2v"/>
                                cuando anochecía,
                                <lb xml:id="l1d94n0w"/>
                                con capa terciada
                                <lb xml:id="l1l5x7qg"/>
                                y espada ceñida.
                                <lb xml:id="l4z6mw2"/>
                                Le seguí los pasos
                                <lb xml:id="leuukxq"/>
                                por ver dónde iba
                                <lb xml:id="l4sx7dw"/>
                                y le vi que entraba
                                <lb xml:id="l1en0t54"/>
                                donde la vecina.
                                <lb xml:id="lw8xnkz"/>
                                Me puse a escuchar
                                <lb xml:id="l568o5f"/>
                                por ver qué decía,
                                <lb xml:id="l1l5lo0o"/>
                                y le oí decir:
                                <lb xml:id="lq73hq1"/>
                                &lt;&lt;Abre, vida mía.&gt;&gt;
                                <lb xml:id="l1bohp7e"/>
                                A ti te he de dar
                                <lb xml:id="l1cy9hyf"/>
                                mantón y mantilla;
                                <lb xml:id="lfgw6u1"/>
                                pero a mi mujer
                                <lb xml:id="l26pi4q"/>
                                palo y mala vida.
                                <lb xml:id="loqrkf8"/>
                                Me volví a mi casa,
                                <lb xml:id="lsx7q51"/>
                                triste y afligida
                                <lb xml:id="l7qd8xl"/>
                                y atranqué la puerta
                                <lb xml:id="lr3u65s"/>
                                con mesas y sillas.
                                <lb xml:id="l1s6j3sy"/>
                                Me puese a coser,
                                <lb xml:id="li5dfz0"/>
                                coser no podía;
                                <lb xml:id="lh812k"/>
                                me puse a bordar,
                                <lb xml:id="lgfesx5"/>
                                bordar no podía.
                                <lb xml:id="lvudugs"/>
                            </dir>
                        </measure>
