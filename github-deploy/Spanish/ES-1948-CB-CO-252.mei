                                <lb xml:id="ls3mdyo"/>
                                quisiera, ir.
                                <lb xml:id="l1hmd6zh"/>
                                En las, montáñaras,
                                <lb xml:id="l1hcf6oe"/>
                                de, Catalúñara,
                                <lb xml:id="l8c28ob"/>
                                y en la, Corúñara,
                                <lb xml:id="l1jbeaoy"/>
                                y en el, Ferrol,
                                <lb xml:id="l1sgc5zv"/>
                                hay, un convéntoro,
                                <lb xml:id="lbum6ka"/>
                                de, religiósaras,
                                <lb xml:id="l1vc3exr"/>
                                que son, hermósaras,
                                <lb xml:id="lihzy1c"/>
                                y no, lo son.
                                <lb xml:id="l14vlds7"/>
                                Ando, buscándoro,
                                <lb xml:id="l1honds1"/>
                                mujer, bonítara,
                                <lb xml:id="ljg871x"/>
                                chuqui, ritítara
                                <lb xml:id="lqmrqzy"/>
                                y no, la hay.
