                            <dir xml:id="dv5rdev" place="below" staff="1" tstamp="1.000000" vgrp="40">
                                Los señoritos llevan
                                <lb xml:id="l12eb2dr"/>
                                en el zapato
                                <lb xml:id="lsnc27b"/>
                                un letrero que dice:
                                <lb xml:id="lewy4vi"/>
                                no tengo un cuarto.
                                <lb xml:id="l17j648d"/>
                                Tengo unas calabazas
                                <lb xml:id="lpcqyyu"/>
                                puestas al humo,
                                <lb xml:id="ljwikg9"/>
                                al primero que pase
                                <lb xml:id="lwzuxsk"/>
                                se las emplumo.
                                <lb xml:id="lhntz16"/>
                                Un alfiler del pecho
                                <lb xml:id="l1oxehii"/>
                                me han regalado,
                                <lb xml:id="l1291rv5"/>
                                y por ser de un tunante
                                <lb xml:id="l10njfa1"/>
                                lo he despreciado.
                                <lb xml:id="l1eaahnu"/>
                                Para ser lechuguino,
                                <lb xml:id="li612kv"/>
                                se necesita
                                <lb xml:id="l1ejtoyl"/>
                                un pantalón de paño
                                <lb xml:id="l274uwu"/>
                                y una levita;
                                <lb xml:id="layegty"/>
                                un bastón de la Habana
                                <lb xml:id="lk3sv4x"/>
                                y un buen sombrero,
                                <lb xml:id="lthhood"/>
                                y un chaleco escotado
                                <lb xml:id="l1d9tzmm"/>
                                de terciopelo,
                                <lb xml:id="l15tnaig"/>
                            </dir>
                            <slur xml:id="szv90pe" startid="#n1h5z3v" endid="#nw3m45m" curvedir="below"/>
