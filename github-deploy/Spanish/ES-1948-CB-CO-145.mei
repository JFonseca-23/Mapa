                            <dir xml:id="dyc8co7" staff="1" tstamp="1.000000" vgrp="40">
                                A edad de siete años
                                <lb xml:id="l1bpyarj"/>
                                la vida quiso dar,
                                <lb xml:id="lh0skzg"/>
                                y la sangre por Cristo
                                <lb xml:id="lydfd7o"/>
                                la quiso derramar.
                                <lb xml:id="l1k8eh0y"/>
                                Al salir por la puerta,
                                <lb xml:id="lk0m5vd"/>
                                se tuvo que parar,
                                <lb xml:id="lfnck3s"/>
                                que le dijo la Virgen,
                                <lb xml:id="l1o90zcm"/>
                                Teresa, ¿dónde vas?
                                <lb xml:id="lrrjohg"/>
                                Yo me voy a los moros,
                                <lb xml:id="l12w2fk6"/>
                                los quiero conquistar.
                                <lb xml:id="l1dd363m"/>
                                No vayas, no, Teresa.
                                <lb xml:id="l1fks1dr"/>
                                que te van a matar;
                                <lb xml:id="lyyflns"/>
                                vete y funda conventos,
                                <lb xml:id="l1r69u61"/>
                                el cielo ganarás,
                                <lb xml:id="l1u25wjd"/>
                                que en la torre más alta
                                <lb xml:id="lr5wf9k"/>
                                la palma encontrarás.
                                <lb xml:id="l1bnzaaj"/>
                            </dir>
